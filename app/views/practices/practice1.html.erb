<script type="text/javascript">
  var dataSet = [5, 10, 15, 20, 25]
  //Select
  //Bind
  //Actions
  //Append
  //text   SBAAT
  //fd function function(d) {return d}
  // you need a function here cus:
    // d has to be managed by something otherwise its undefined parameter
    // a function is adequate cus It allows you to do additionnal operation with the data
  //fd can be chained with everything there is !! style, text, etc
  d3.select("body").selectAll("p")
    .data(dataSet)
    .enter()
    .append("p")
    .text(function(d) {return "I can count up to " + d})
    .style("color", function(d){
      if (d > 15){
        return "red"
      } else{
        return "blue"
      }
    });

  console.log(d3.selectAll("p"))
</script>
